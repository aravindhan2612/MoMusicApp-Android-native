            android:id="@+id/action_mainFragment_to_playerFragment2"
            app:destination="@id/playerFragment2" />
    </fragment>
    <fragment
        android:id="@+id/playerFragment2"
        android:name="com.example.jsmusicapp.fragments.PlayerFragment"
        android:label="player_fragment"
        tools:layout="@layout/player_fragment" />
